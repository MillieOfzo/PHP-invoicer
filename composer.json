{
    "name": "millieofzo/php-invoicer",
    "description": "Library to generate PDF invoices with PHP",
    "type": "library",
    "license": "MIT",
    "keywords": ["invoice", "pdf", "php", "MillieOfzo"],
    "support": {
        "source": "https://github.com/MillieOfzo/PHP-invoicer"
    },
    "authors": [
        {
            "name": "Roelof Jan van Golen",
            "homepage": "https://github.com/MillieOfzo"
        }
    ],
    "require": {
        "php": ">=5.6",
        "setasign/fpdf": "~1.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": { "MillieOfzo\\PHPInvoicer\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "MillieOfzo\\PHPInvoicer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
