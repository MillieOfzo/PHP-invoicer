{
    "name": "MillieOfzo/pdf-invoice",
    "description": "Library to generate PDF invoices",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "keywords": ["invoice", "pdf", "MillieOfzo", "artkonekt"],
    "support": {
        "source": "https://github.com/artkonekt/pdf-invoice"
    },
    "authors": [
        {
            "name": "Roelof Jan van Golen",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": ">=5.6",
        "setasign/fpdf": "~1.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": { "MillieOfzo\\PHPInvoicer\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "MillieOfzo\\PHPInvoicer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
